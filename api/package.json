{
  "name": "react-ts-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "NODE_ENV=test nodemon --watch './src/**/*.ts' --exec ts-node --files -r ./src/server.ts",
    "test": "jest --config ./jest.config.js",
    "test:unit": "jest --config ./jest.config.js --watch",
    "build": "rm -rf dist && tsc -p tsconfig.prod.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --config .prettierrc --write ."
  },
  "dependencies": {
    "@types/pg": "^8.11.2",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@jest-mock/express": "^2.0.2",
    "@swc/core": "^1.3.96",
    "@swc/jest": "^0.2.29",
    "@types/bcrypt": "^5.0.1",
    "@types/cookie-parser": "^1.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "29.5.7",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.8",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "express-jwt": "^8.4.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "3.2.5",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "zod": "^3.22.4"
  }
}
